{
  "name": "votemike/money",
  "description": "A class for dealing with Money (a combination of a value and a currency)",
  "keywords": [
    "Money"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Gwynne",
      "email": "michael@votemike.co.uk"
    }
  ],
  "require": {
    "symfony/intl": "^2.0 || ^3.0"
  },
  "require-dev": {
    "phpbench/phpbench": "^0.13.0",
    "phpunit/phpunit": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Votemike\\Money\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Votemike\\Money\\Test\\": "tests"
    }
  },
  "prefer-stable": true,
  "sort-packages": true
}
